{
  "name": "@feyond/console-logging",
  "version": "1.1.5",
  "description": "Browser console log printing wrapper supports log level control.",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rimraf ./lib && npm run build:types && npm run build:js",
    "build:js": "babel src --out-dir ./lib --extensions \".ts\"",
    "build:vite": "vite build --mode development && vite build --mode production && npm run build:types",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir ./lib",
    "prepublish": "npm run build",
    "test": "jest",
    "lint": "eslint --ext .ts",
    "lint:fix": "eslint --fix --ext .ts"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.10",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/register": "^7.18.9",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.7.1",
    "@types/readable-stream": "^2.3.14",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "babel-jest": "^28.1.3",
    "babel-plugin-const-enum": "^1.2.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-ts-optchain": "^1.1.5",
    "coveralls": "^3.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "standard-version": "^9.5.0",
    "typescript": "^4.7.4"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "homepage": "https://github.com/feyond/console-logging",
  "bugs": {
    "url": "https://github.com/feyond/console-logging/issues",
    "email": "cfyond@gmail.com"
  },
  "keywords": [
    "console.log",
    "console.debug",
    "console.error",
    "console.info",
    "console",
    "logging"
  ],
  "author": "feyond",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/feyond/console-logging.git"
  },
  "dependencies": {
    "@babel/runtime": "^7.18.9",
    "core-js": "^3.24.1",
    "fecha": "^4.2.3",
    "readable-stream": "^3.6.0"
  }
}
