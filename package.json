{
  "name": "@feyond/console-logging",
  "version": "1.1.3",
  "description": "Browser console log printing wrapper supports log level control.",
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "type": "module",
  "typings": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "lib/index.es.js",
      "require": "lib/index.cjs.js"
    }
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "vite build --mode development && vite build --mode production && npm run build:types",
    "build:types": "tsc src/index.ts --declaration --emitDeclarationOnly --outDir ./lib",
    "prepublish": "npm run build",
    "test": "jest",
    "lint": "eslint --ext .ts",
    "lint:fix": "eslint --fix --ext .ts"
  },
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/node": "^18.7.1",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.3",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "vite": "^3.0.5"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "homepage": "https://github.com/feyond/console-logging",
  "bugs": {
    "url": "https://github.com/feyond/console-logging/issues",
    "email": "cfyond@gmail.com"
  },
  "keywords": [
    "console.log",
    "console.debug",
    "console.error",
    "console.info",
    "console",
    "logging"
  ],
  "author": "feyond",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/feyond/console-logging.git"
  }
}
